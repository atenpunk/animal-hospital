<?xml version="1.0" encoding="UTF-8"?>
<pretty-config xmlns="http://ocpsoft.com/prettyfaces/2.0.4"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://ocpsoft.com/prettyfaces/2.0.4
        
        http://ocpsoft.com/xml/ns/prettyfaces/ocpsoft-pretty-faces-2.0.4.xsd">
    
    
        <url-mapping id="root">
        <pattern value="/"></pattern>
        <view-id>/sec/home.jsf</view-id>
    </url-mapping>
    
    <url-mapping id="login">
        <pattern value="/login"></pattern>
        <view-id>/login.jsf</view-id>
    </url-mapping>
           
    <url-mapping id="logout">
        <pattern value="/logout"></pattern>
        <view-id >/login.jsf</view-id>
        <action>#{identity.logout}</action>
    </url-mapping>
    
    <url-mapping id="denied">
        <pattern value="/denied"></pattern>
        <view-id>/denied.jsf</view-id>
    </url-mapping>
    
     <url-mapping id="passwd">
        <pattern value="/user/passwd"></pattern>
        <view-id>/sec/user/passwd.jsf</view-id>
    </url-mapping>
    
	<url-mapping id="subscriber_list">
		<pattern value="/subscribers"></pattern>
		<view-id>/sec/subscriber/list.jsf</view-id>
	</url-mapping>
	<url-mapping id="subscriber_view">
		<pattern value="/subscribers/#{id:subscriberController.strSubscriberId}"></pattern>
		<view-id>/sec/subscriber/view.jsf</view-id>
		<action>#{subscriberController.loadSubscriber()}</action>      
	</url-mapping>
	<url-mapping id="subscriber_edit">
		<pattern value="/subscribers/edit/#{id:subscriberController.strSubscriberId}"></pattern>
		<view-id>/sec/subscriber/edit.jsf</view-id>
		<action>#{subscriberController.loadEditSubscriber()}</action>      
	</url-mapping>

	<url-mapping id="station_list">
		<pattern value="/stations"></pattern>
		<view-id>/sec/station/list.jsf</view-id>
		<action>#{stationController.find()}</action>
	</url-mapping>
	<url-mapping id="station_view">
		<pattern value="/stations/#{id:stationController.strStationId}"></pattern>
		<view-id>/sec/station/view.jsf</view-id>
		<action>#{stationController.loadStation()}</action>      		
	</url-mapping>
	
	<url-mapping id="Display_Total_Amount">
		<pattern value="/displayTotalAmounts"></pattern>
		<view-id>/sec/displayTotalAmount/list.jsf</view-id>
	</url-mapping>	
	
	<url-mapping id="tariffTable_list">
		<pattern value="/tariffTable"></pattern>
		<view-id>/sec/tariffTable/list.jsf</view-id>
		<action>#{tariffController.run()}</action>
	</url-mapping>
	<url-mapping id="tariffTable_edit">
		<pattern value="/tariffTable/edit"></pattern>
		<query-param name="effectiveDate"> #{tariffController.effectiveDate } </query-param>
		<query-param name="hour"> #{tariffController.hourStr } </query-param>
		<view-id>/sec/tariffTable/edit.jsf</view-id>
		<action>#{tariffController.loadEditTariff()}</action>      
	</url-mapping>
	
	<url-mapping id="sale_store">
		<pattern value="/salesInformations"></pattern>
		<view-id>/sec/salesInformation/salesInfoList.jsf</view-id>
		<action>#{saleControll.run()}</action>   
	</url-mapping>	
	
	<url-mapping id="tariffMode_list">
		<pattern value="/tariffMode"></pattern>
		<view-id>/sec/tariffMode/list.jsf</view-id>
		<action>#{tariffModeController.run()}</action>
	</url-mapping>
	<url-mapping id="main_formTopup">
		<pattern value="/topupList"></pattern>
		<view-id>/sec/topupInformation/topupList.jsf</view-id>
	</url-mapping>
	<url-mapping id="main_formCancelTopup">
		<pattern value="/cancelTopup"></pattern>
		<view-id>/sec/topupInformation/cancelTopup.jsf</view-id>
		<action>#{cancelTopupControll.run()}</action>
	</url-mapping>
	<url-mapping id="main_formCB">
		<pattern value="/checkBalance"></pattern>
